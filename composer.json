{
    "name": "winwin/mapper-generator",
    "description": "Generator for object property mapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "winwin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "winwin\\mapper\\": ["src/", "lib/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "winwin\\mapper\\": "tests/"
        }
    },
    "scripts": {
        "lint": "@php -d memory_limit=-1 ./vendor/bin/phpstan analyze --error-format=raw"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "kuiper/serializer": "^0.8",
        "nikic/php-parser": "^5.0",
        "psr/log": "^1.0||^2.0||^3.0",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "winwin/composer-phpcs-plugin": "^0.4"
    },
    "bin": ["bin/mapper-generator", "bin/builder-generator"]
}
